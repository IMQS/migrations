#######################################################################################################################################
# SCADA tables used to store historical, location and current values for SCADA sensors
#######################################################################################################################################

CREATE TABLE "ScadaMain" ScadaMain
{
    "Internal"
    required    int64               rowid                   ""


    "Sensor Data"
    optional    text                Tag                     "Tag"
    optional    text                Name                    "Name"
    optional    int64               LocationID              "Location ID"
    optional    int64               UnitID                  "Unit ID"
    optional    bool                DefaultSensor           "Default Sensor"
    optional    double              Intercept               "Intercept"
    optional    double              Gradient                "Gradient"
    optional    int64               SensorTypeID            "Sensor Type ID"
    optional    double              WarningLow              "Low Warning"
    optional    double              WarningHigh             "High Warning"
    optional    double              LimitLow                "Low Limit"
    optional    double              LimitHigh               "High Limit"
    optional    bool                Enabled                 "Enabled"

    hasone  Current Tag ScadaCurrent.Tag
    hasone  Location LocationID ScadaLocation.rowid
    hasone  Unit UnitID ScadaUnit.rowid
    hasone  SensorType SensorTypeID ScadaSensorType.rowid
}
PRIMARY KEY(rowid)
UNIQUE INDEX(Tag)
INDEX(UnitID)
INDEX(LocationID)
INDEX(SensorTypeID);

